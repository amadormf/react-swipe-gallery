    "jsdom": "^8.3.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
