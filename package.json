  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.4",
    "prop-types": "^15.6.0",
    "react-swipeable": "^4.2.0"
  },
  "main": "lib/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "pre-commit": {
    "run": "lint, test"
  }
