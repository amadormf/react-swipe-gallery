{
  "name": "react-swipe-gallery",
  "version": "0.6.0",
  "description": "React Swipe Gallery Component",
  "repository": {
    "type": "git",
    "url": "https://github.com/amadormf/react-swipe-gallery.git"
  },
  "license": "MIT",
  "scripts": {
    "build": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
